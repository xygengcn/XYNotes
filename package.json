{
  "name": "xynotes",
  "version": "3.0.3",
  "description": "XY笔记本是XY博客新尝试的一个轻量级的本地笔记本，是一款基于vue框架的笔记本，数据存储本地浏览器，默认支持markdown语法，支持图标数学等语法，支持即时渲染，支持截图分享等。",
  "keywords": [
    "vue2",
    "typescript",
    "tsx",
    "pinia",
    "notepad",
    "vditor"
  ],
  "private": true,
  "scripts": {
    "dev": "vite --port=8080",
    "build": "vite build",
    "start": "vite preview"
  },
  "dependencies": {
    "@vue/composition-api": "^1.6.2",
    "html2canvas": "^1.4.1",
    "js-lark": "^0.0.2",
    "pinia": "^2.0.14",
    "register-service-worker": "^1.7.2",
    "ts-database": "0.0.3",
    "vditor": "^3.8.15",
    "vue": "^2.6.14",
    "vue-router": "^3.5.4"
  },
  "devDependencies": {
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "prettier": "^2.6.2",
    "sass": "^1.52.1",
    "typescript": "^4.6.4",
    "vite": "^2.9.8",
    "vite-plugin-pwa": "^0.12.0",
    "vite-plugin-vue2": "^2.0.0",
    "vue-class-component": "^7.2.6",
    "vue-property-decorator": "^9.1.2",
    "vue-template-compiler": "^2.6.14"
  }
}
