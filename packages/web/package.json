    "@tauri-apps/plugin-dialog": "2.0.0-beta.3",
    "@tauri-apps/plugin-fs": "2.0.0-rc.2",
    "click-outside-vue3": "^4.0.1",
    "html2canvas": "^1.4.1",
    "js-lark": "^0.0.2",
    "overlayscrollbars": "^2.4.5",
    "overlayscrollbars-vue": "^0.5.6",
    "pinia": "^2.1.7",
    "register-service-worker": "^1.7.2",
    "ts-database": "0.0.4",
    "vditor-lite": "3.9.8",
    "vue-tippy": "^6.3.1",
    "vue3-swipe-actions": "^0.0.1"
